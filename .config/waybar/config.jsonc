{
    "spacing": 8,
    "margin-top": 12,
    "modules-left": [
        "tray",
        "hyprland/workspaces"
    ],
    "modules-center": [
        "hyprland/window"
    ],
    "modules-right": [
        "idle_inhibitor",
        "pulseaudio",
        "network",
        "cpu",
        "memory",
        "backlight",
        "battery",
        "clock"
    ],
    "hyprland/workspaces": {
        "disable-scroll": true,
        "all-outputs": true,
        "warp-on-scroll": true,
        "format": "{name}",
        "persistent-workspaces": {
            "*": 9
        }
    },
    "hyprland/window": {
        "format": "{title}",
        "rewrite": {
            "(.*) ‚Äî Mozilla Firefox": "Ôâ© ‚Äî $1",
            "yazi: (.*)": "Ó´¨ ‚Äî $1",
            "btop(.*)": "Ôíº",
            "(.*) - Discord": "Ôáø",
            "tmux (.*)": "Ôíâ ‚Äî $1",
            "(.*) - mpv": "ÔÄΩ ‚Äî $1",
            "kitty(.*)": "Ôíâ ‚Äî $1",
            "steam(.*)": "ÔÜ∂",
            "imv - (.*)": "ÔÄæ ‚Äî $1",
            "(.*) - Obsidian v(.*)": "Ó¨¶ ‚Äî $1",
            "Obsidian": "Ó¨¶",
            "Obsidian(.*)": "Ó¨¶ ‚Äî $1"
        }
    },
    "keyboard-state": {
        "numlock": true,
        "capslock": true,
        "format": "{name} {icon}",
        "format-icons": {
            "locked": "ÔÄ£",
            "unlocked": "ÔÇú"
        }
    },
    "sway/mode": {
        "format": "<span style=\"italic\">{}</span>"
    },
    "sway/scratchpad": {
        "format": "{icon} {count}",
        "show-empty": false,
        "tooltip": true,
        "tooltip-format": "{app}: {title}"
    },
    "idle_inhibitor": {
        "format": "{icon}",
        "format-icons": {
            "activated": "ÔÅÆ",
            "deactivated": "ÔÅ∞"
        }
    },
    "tray": {
        "icon-size": 20,
        "spacing": 10
    },
    "clock": {
        "timezone": "Europe/Copenhagen",
        "tooltip": true,
        "tooltip-format": "<big>{:%Y %B}</big>\n<tt><small>{calendar}</small></tt>",
        "format-alt": "{:%Y-%m-%d}"
    },
    "cpu": {
        "format": "{usage}% Ôãõ",
        "tooltip": false
    },
    "memory": {
        "format": "{}% ÔÉâ"
    },
    "temperature": {

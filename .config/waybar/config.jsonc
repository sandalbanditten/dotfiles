        "format-icons": [
            "Ôù´",
            "Ôãâ",
            "Ôù©"
        ]
    },
    "backlight": {
        "format": "{percent}% {icon}",
        "format-icons": [
            "Óéç",
            "Óèì",
            "Óèë",
            "Óèè",
            "Óèé",
            "Óèç",
            "Óèä",
            "Óèà",
            "Óéõ"
        ],
        "scroll-step": 5
    },
    "battery": {
        "states": {
            "warning": 30,
            "critical": 15
        },
        "format": "{capacity}% {icon}",
        "format-full": "{capacity}% {icon}",
        "format-charging": "{capacity}% Ôóß",
        "format-plugged": "{capacity}% Ôá¶",
        "format-alt": "{time} {icon}",
        "format-icons": [
            "ÔâÑ",
            "ÔâÉ",
            "ÔâÇ",
            "ÔâÅ",
            "ÔâÄ"
